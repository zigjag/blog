<%- include("partials/header") -%>

<a class="btn btn-primary" href="/compose">Compose</a>
<h1><%= journalTitle %></h1>
<p><%= startingContent %></p>

<%  posts.forEach(function(post){ %>

<h2><%=post.title%></h2>
<p>
  <%=post.content.substring(0, 100) + " ..."%>
  <a href="/posts/<%=post._id%>">Read More</a>
</p>


<% }) %>

<% -include("partials/footer") -%>
