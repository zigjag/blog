<%- include("partials/header") -%>

<a class="btn btn-primary" href="/login?title=<%= journalTitle %>">Compose</a>
<h1><%= headerTitle %></h1>
<p><%= startingContent %></p>

<%  posts.forEach(function(post){ %>

<h2><%=post.title%></h2>
<p>
  <%=post.content.substring(0, 100) + " ..."%>
  <a href="/<%= journalTitle %>/<%=post._id%>">Read More</a>
</p>


<% }) %>

<% -include("partials/footer") -%>
